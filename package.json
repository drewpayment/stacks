{
  "name": "kitforstartups",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "NODE_ENV=production vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "generate-migrations:mysql": "drizzle-kit generate:mysql --config=src/lib/drizzle/mysql/drizzle.config.ts",
    "generate-migrations:postgres": "drizzle-kit generate:pg --config=src/lib/drizzle/postgres/drizzle.config.ts",
    "generate-migrations:turso": "drizzle-kit generate:turso --config=src/lib/drizzle/turso/drizzle.config.ts",
    "push:mysql": "drizzle-kit push:mysql --config=src/lib/drizzle/mysql/drizzle.config.ts",
    "push:postgres": "drizzle-kit push:pg --config=src/lib/drizzle/postgres/drizzle.config.ts",
    "push:turso": "drizzle-kit push:turso --config=src/lib/drizzle/turso/drizzle.config.ts",
    "migrate:mysql": "tsx src/lib/drizzle/mysql/migrations/migrate.ts",
    "migrate:drop": "drizzle-kit drop --config=src/lib/drizzle/mysql/drizzle.config.ts",
    "migrate:postgres": "tsx src/lib/drizzle/postgres/migrations/migrate.ts",
    "migrate:turso": "tsx src/lib/drizzle/turso/migrations/migrate.ts",
    "studio:mysql": "drizzle-kit studio --config=src/lib/drizzle/mysql/drizzle.config.ts",
    "studio:postgres": "drizzle-kit studio --config=src/lib/drizzle/postgres/drizzle.config.ts",
    "vi:test": "vitest run",
    "coverage": "vitest run --coverage",
    "vi:test:ui": "vitest --ui",
    "vi:test:watch": "vitest",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:report": "playwright show-report",
    "docker:build": "docker build --pull --rm -f 'Dockerfile' -t stacks:latest --build-arg ORIGIN=https://stacks.drewpayment.com '.'"
  },
  "devDependencies": {
    "@iconify/svelte": "^3.1.6",
    "@melt-ui/pp": "^0.1.4",
    "@playwright/test": "^1.44.1",
    "@sveltejs/adapter-auto": "^3.0.1",
    "@sveltejs/adapter-node": "^4.0.1",
    "@sveltejs/kit": "^2.5.10",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/svelte": "^5.1.0",
    "@types/jest": "^29.5.11",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.12.12",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "autoprefixer": "^10.4.16",
    "bits-ui": "^0.21.16",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.19.13",
    "emailjs": "^4.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-svelte": "^2.35.1",
    "formsnap": "^1.0.1",
    "jsdom": "^22.1.0",
    "lucide-svelte": "^0.344.0",
    "msw": "^2.3.1",
    "pg": "^8.11.5",
    "postcss": "^8.4.32",
    "postcss-load-config": "^4.0.2",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "svelte-sequential-preprocessor": "^2.0.1",
    "sveltekit-superforms": "^2.21.0",
    "tailwind-merge": "^2.5.5",
    "tailwind-variants": "^0.3.0",
    "tailwindcss": "^3.4.0",
    "tslib": "^2.6.2",
    "tsx": "^3.14.0",
    "typescript": "^5.3.3",
    "vite": "^5.2.12",
    "vitest": "^1.6.0",
    "vitest-dom": "^0.1.1",
    "zod": "^3.22.4"
  },
  "type": "module",
  "dependencies": {
    "@fontsource-variable/inter": "^5.0.16",
    "@libsql/client": "^0.3.6",
    "@lucia-auth/adapter-mysql": "^3.0.0",
    "@lucia-auth/adapter-postgresql": "^3.1.0",
    "@lucia-auth/adapter-sqlite": "^3.0.0",
    "@lucia-auth/oauth": "^3.5.3",
    "@melt-ui/svelte": "^0.49.1",
    "@planetscale/database": "^1.13.0",
    "@popperjs/core": "^2.11.8",
    "@tailwindcss/forms": "^0.5.7",
    "cmdk-sv": "^0.0.18",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.1",
    "drizzle-orm": "^0.28.6",
    "flowbite": "^2.2.1",
    "flowbite-svelte": "^0.44.21",
    "flowbite-svelte-blocks": "^0.5.1",
    "flowbite-svelte-icons": "^0.4.5",
    "input-core": "^2.1.0",
    "lucia": "^3.0.1",
    "mysql2": "^3.6.5",
    "nanoid": "^4.0.2",
    "oslo": "^1.1.3",
    "postgres": "^3.4.3",
    "resend": "^1.1.0",
    "svelte-hero-icons": "^5.2.0",
    "svelte-input-mask": "^1.1.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.1/xlsx-0.20.1.tgz"
  }
}
