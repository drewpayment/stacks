{
	"name": "kitforstartups",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"generate-migrations:mysql": "drizzle-kit generate:mysql --config=src/lib/drizzle/mysql/drizzle.config.ts",
		"generate-migrations:postgres": "drizzle-kit generate:pg --config=src/lib/drizzle/postgres/drizzle.config.ts",
		"generate-migrations:turso": "drizzle-kit generate:turso --config=src/lib/drizzle/turso/drizzle.config.ts",
		"push:mysql": "drizzle-kit push:mysql --config=src/lib/drizzle/mysql/drizzle.config.ts",
		"push:postgres": "drizzle-kit push:pg --config=src/lib/drizzle/postgres/drizzle.config.ts",
		"push:turso": "drizzle-kit push:turso --config=src/lib/drizzle/turso/drizzle.config.ts",
		"migrate:mysql": "tsx src/lib/drizzle/mysql/migrations/migrate.ts",
		"migrate:drop": "drizzle-kit drop --config=src/lib/drizzle/mysql/drizzle.config.ts",
		"migrate:postgres": "tsx src/lib/drizzle/postgres/migrations/migrate.ts",
		"migrate:turso": "tsx src/lib/drizzle/turso/migrations/migrate.ts",
		"studio": "drizzle-kit studio --config=src/lib/drizzle/mysql/drizzle.config.ts",
		"test": "vitest run",
		"coverage": "vitest run --coverage",
		"test:ui": "vitest --ui",
		"test:watch": "vitest"
	},
	"devDependencies": {
		"@iconify/svelte": "^3.1.4",
		"@melt-ui/pp": "^0.1.2",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.20.4",
		"@testing-library/svelte": "^4.0.3",
		"@types/jest": "^29.5.5",
		"@types/jsdom": "^21.1.3",
		"@types/pg": "^8.10.2",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"@vitest/coverage-v8": "^0.34.6",
		"@vitest/ui": "^0.34.6",
		"autoprefixer": "^10.4.14",
		"drizzle-kit": "^0.19.13",
		"emailjs": "^4.0.3",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"jest-dom": "^4.0.0",
		"jsdom": "^22.1.0",
		"postcss": "^8.4.24",
		"postcss-load-config": "^4.0.1",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"svelte": "^4.2.1",
		"svelte-check": "^3.5.2",
		"svelte-sequential-preprocessor": "^2.0.1",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.6.2",
		"tsx": "^3.13.0",
		"typescript": "^5.2.2",
		"vite": "^4.4.9",
		"vitest": "^0.34.6",
		"vitest-dom": "^0.1.1"
	},
	"type": "module",
	"dependencies": {
		"@fontsource-variable/inter": "^5.0.8",
		"@libsql/client": "^0.3.2",
		"@lucia-auth/adapter-mysql": "^2.0.0",
		"@lucia-auth/adapter-postgresql": "^2.0.1",
		"@lucia-auth/adapter-sqlite": "^2.0.0",
		"@lucia-auth/oauth": "^3.0.0",
		"@melt-ui/svelte": "^0.49.1",
		"@planetscale/database": "^1.11.0",
		"@tailwindcss/forms": "^0.5.6",
		"dotenv": "^16.3.1",
		"drizzle-orm": "^0.28.5",
		"input-core": "^2.0.1",
		"lucia": "^2.4.2",
		"lucide-svelte": "^0.279.0",
		"mysql2": "^3.6.0",
		"nanoid": "^4.0.2",
		"postgres": "^3.3.5",
		"resend": "^1.0.0",
		"svelte-input-mask": "^1.1.0",
		"zod": "^3.22.2"
	}
}
